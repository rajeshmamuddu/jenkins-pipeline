pipeline {
  agent 
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn clean install package'
      }
    }
    stage('build') {
      agent {
        docker { image 'pengbai/docker-supermario' }
      }
      steps {
        sh 'echo penguin image'
      }
    }
    stage('deploy') {
      agent {
        docker { image 'pengbai/docker-supermario' }
      }
      steps {
        sh 'echo hello rajesh'
      }
    }
  }
}
